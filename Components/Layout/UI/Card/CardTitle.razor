<h3 @attributes=AdditionalAttributes class="text-2xl font-semibold leading-none tracking-tight @(AdditionalAttributes?["class"] ?? "")">
    @ChildContent
</h3>

@code {
    [Parameter]
    public RenderFragment? ChildContent { get; set; }

    [Parameter(CaptureUnmatchedValues = true)]
    public IDictionary<string, object>? AdditionalAttributes { get; set; }
}